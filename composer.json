{
    "name": "rodrigorioo/backstrap-laravel",
    "description": "A Laravel integration for backstrap",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rodrigo Rio",
            "email": "rodrigo.rio@outlook.com.ar"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=7.4.10",
        "illuminate/support": ">=4.0",
        "laravelcollective/html": "^6.3",
        "yajra/laravel-datatables-oracle": "^10.1",
        "spatie/laravel-permission": "dev-main",
        "spatie/laravel-translatable": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Rodrigorioo\\BackStrapLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rodrigorioo\\BackStrapLaravel\\BackStrapLaravelServiceProvider"
            ],
            "aliases": {
                "BackStrapLaravel": "Rodrigorioo\\BackStrapLaravel\\Facades\\BackStrapLaravel"
            }
        }
    }
}
